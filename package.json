{
    "name": "Alure",
    "version": "1.0.0",
    "private": true,
    "description": "Alure",
    "devDependencies": {
        "autoprefixer": "8.3.x",
        "browser-sync": "2.23.x",
        "gulp": "4.0.x",
        "gulp-csso": "^2.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "7.0.x",
        "gulp-sass": "^4.0.2",
        "gulp-typograf": "^3.1.0"
    },
    "scripts": {
        "editorconfig": "editorconfig-cli",
        "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
        "test": "npm run editorconfig && npm run stylelint",
        "build": "gulp style",
        "start": "gulp serve",
        "typo": "gulp typograf"
    },
    "browserslist": [
        "last 10 versions",
        "IE 11",
        "Firefox ESR"
    ],
    "editorconfig-cli": [
        "*.json",
        "*.js",
        "source/*.html",
        "source/js/**/*.js",
        "source/img/**/*.svg",
        "source/sass/**/*.{sass,scss}"
    ],
    "engines": {
        "node": "8.11"
    },
    "dependencies": {
        "gh-pages": "^2.1.1",
        "npm": "^6.13.4",
        "path": "^0.12.7",
        "svgo": "^1.3.2",
        "tipograf": "^2.0.0",
        "typograf": "^6.10.0"
    }
}
